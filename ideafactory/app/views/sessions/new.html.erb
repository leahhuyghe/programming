<h2>Sign in</h1>


  <%= form_tag sessions_path do %>
  <div>
    <%= label_tag :email %>
    <%= email_field_tag :email %>
  </div>

  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>

  <div>
    <%= submit_tag "Sign In", class: "btn btn-info"  %>
  </div>
<% end %>
